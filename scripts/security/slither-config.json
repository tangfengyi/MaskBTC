{
  "detectors_to_exclude": [
    "unused-state",
    "solc-version",
    "external-function"
  ],
  "filter_paths": [
    "test/",
    "mocks/"
  ],
  "compilation_units": {
    "ethereum_contracts": {
      "files": [
        "maskly/blockchain/contracts/**/*.sol"
      ],
      "remappings": [
        "@openzeppelin/=node_modules/@openzeppelin/"
      ],
      "solc_version": "0.8.17",
      "optimizer": true,
      "optimizer_runs": 200
    },
    "binance_contracts": {
      "files": [
        "maskly/blockchain/contracts/bridgeCore/protocols/Lock-Mint/Binance/*.sol"
      ],
      "remappings": [
        "@openzeppelin/=node_modules/@openzeppelin/"
      ],
      "solc_version": "0.8.17",
      "optimizer": true,
      "optimizer_runs": 200
    }
  }
}

